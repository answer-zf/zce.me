{
  "name": "zce.me",
  "version": "0.1.0",
  "private": true,
  "description": "zceâ€™s personal homepage",
  "keywords": [
    "homepage",
    "zce.me",
    "zce"
  ],
  "homepage": "https://github.com/zce/zce.me#readme",
  "bugs": {
    "url": "https://github.com/zce/zce.me/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zce/zce.me.git"
  },
  "license": "MIT",
  "author": {
    "name": "zce",
    "email": "w@zce.me",
    "url": "https://zce.me"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,json,yml,yaml,md,scss}\"",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "dist": "yarn build --prefix-paths",
    "dev": "yarn develop",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,json,yml,yaml,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "endOfLine": "lf",
    "jsxBracketSameLine": true,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "disqus-react": "1.0.7",
    "gatsby": "2.15.20",
    "gatsby-image": "2.2.20",
    "gatsby-plugin-nprogress": "2.1.7",
    "gatsby-plugin-react-helmet": "3.1.8",
    "gatsby-plugin-sass": "2.1.15",
    "gatsby-plugin-sharp": "2.2.25",
    "gatsby-remark-autolink-headers": "2.1.11",
    "gatsby-remark-copy-linked-files": "2.1.20",
    "gatsby-remark-images": "3.1.23",
    "gatsby-remark-prismjs": "3.3.14",
    "gatsby-remark-responsive-iframe": "2.2.17",
    "gatsby-remark-smartypants": "2.1.9",
    "gatsby-source-filesystem": "2.1.26",
    "gatsby-transformer-remark": "2.6.24",
    "gatsby-transformer-sharp": "2.2.16",
    "gatsby-transformer-yaml": "2.2.10",
    "husky": "3.0.5",
    "lint-staged": "9.2.5",
    "lodash": "4.17.15",
    "node-sass": "4.12.0",
    "normalize.css": "8.0.1",
    "open-color": "1.7.0",
    "prettier": "1.18.2",
    "prismjs": "1.17.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1"
  }
}
